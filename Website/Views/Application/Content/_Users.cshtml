<div id="usersContent">
    <h1 class="page-title">@Resource.Common_Users</h1>
    <div class="row">
        <div class="col-sm-4">
            <div class="list-group">
                <div class="list-group-item" ng-repeat="user in users" ng-class="{ active: isActive(user)}" ng-click="changeUser(user)">
                    {{ user.UserName }}
                </div>
            </div>
        </div>
        <div class="col-sm-8">
        </div>
    </div>
</div>